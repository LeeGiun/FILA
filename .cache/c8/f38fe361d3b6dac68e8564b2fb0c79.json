5px;\r\n  height: 25px;\r\n  \r\n}\r\n\r\n\r\n\r\n/* lnb */\r\n.lnb {\r\n  width: 100%;\r\n  height: 100px;\r\n}\r\n.lnb_box {\r\n  position: relative;\r\n  z-index: 2;\r\n  width: 400px;\r\n  height: 100px;\r\n  margin: 0 auto;\r\n  display: flex;\r\n  justify-content: space-evenly;\r\n  flex-direction: row;\r\n}\r\n.lnb_box li {\r\n  height: 100px;\r\n  line-height: 100px;\r\n  margin-right: 20px;\r\n}\r\n.lnb_box li a {\r\n  color: #000;\r\n  font-weight: bold;\r\n  font-size: 18px;\r\n}\r\n.lnb_box .bar {\r\n  width: 2px;\r\n  height: 12px;\r\n  border-right: 1px solid #000;\r\n  margin-top: 44px;\r\n}\r\n\r\n\r\n\r\n\r\n\r\n/* ========================== */\r\n\r\n\r\n/* 타이틀 */\r\n.IdPw_main {\r\n  width: 100%;\r\n}\r\n.IdPw_main_box {\r\n  width: 1440px;\r\n  margin: 0 auto;\r\n}\r\n.IdPw_main_box h1 {\r\n  width: 400px;\r\n  height: 100px;\r\n  margin: 0 auto;\r\n  text-align: center;\r\n  line-height: 100px;\r\n  box-sizing: border-box;\r\n}\r\n\r\n\r\n/* 카카오 간편 */\r\n.kakao_box {\r\n  width: 600px;\r\n  height: 110px;\r\n  margin: 0 auto;\r\n  margin-top: 50px;\r\n  margin-bottom: 50px;\r\n}\r\n.kakao_box h4 {\r\n  font-weight: normal;\r\n}\r\n.kakao_box input {\r\n  display: none;\r\n}\r\n.kakao_box label {\r\n  display: block;\r\n  width: 600px;\r\n  height: 60px;\r\n  text-align: center;\r\n  line-height: 60px;\r\n  font-size: 18px;\r\n  border-radius: 30px;\r\n  background-color: rgb(255, 220, 0);\r\n  background-image: url(./back_img/kakao_13x12.png);\r\n  background-repeat: no-repeat;\r\n  background-position-y: center;\r\n  background-position-x: 23px;\r\n  margin-top: 30px;\r\n}\r\n\r\n\r\n\r\n\r\n\r\n\r\n/* 이름, 휴대폰번호 입력 */\r\n.IdPw_box1 {\r\n  width: 600px;\r\n  height: 130px;\r\n  margin: 0 auto;\r\n  margin-top: 50px;\r\n}\r\n.IdPw_box1 input{\r\n  display: flex;\r\n  width: 600px;\r\n  height: 50px;\r\n  margin: 0 auto;\r\n  border-radius: 5px;\r\n  padding: 10px 15px;\r\n  box-sizing: border-box;\r\n  margin-bottom: 10px;\r\n  outline: none;\r\n  border: 1px solid #ccc;\r\n}\r\n\r\n#IdPw_pw {\r\n  width: 335px;\r\n  float: right;\r\n}\r\n.IdPw_box1 #IdPw_Tel1,\r\n.IdPw_box1 #IdPw_Tel2 {\r\n  width: 120px;\r\n  height: 50px;\r\n  outline: none;\r\n  border: 1px solid #ccc;\r\n  padding: 10px;\r\n  color: #888;\r\n  box-sizing: border-box;\r\n  margin-right: 10px;\r\n  border-radius: 5px;\r\n}\r\n.IdPw_box1 #IdPw_Tel3 {\r\n  width: 335px;\r\n  float: right;\r\n}\r\n\r\n\r\n\r\n\r\n/* 인증번호 요청 버튼 */\r\n/* 인증번호 확인 입력창 */\r\n.IdPw_btn,\r\n.IdPw_btnChk {\r\n  width: 600px;\r\n  height: 50px;\r\n  margin: 0 auto;\r\n}\r\n.IdPw_btnChk {\r\n  display: flex;\r\n  margin-top: 50px;\r\n  margin-bottom: 150px;\r\n}\r\n.IdPw_btn input {display: none;}\r\n.IdPw_btn label {\r\n  display: block;\r\n  width: 600px;\r\n  height: 60px;\r\n  text-align: center;\r\n  line-height: 60px;\r\n  background-color: #002965;\r\n  border: 1px solid #002965;\r\n  border-radius: 40px;\r\n  cursor: pointer;\r\n  color: #fff;\r\n  font-weight: bold;\r\n  font-size: 17px;\r\n  box-sizing: border-box;\r\n  margin-top: 5px;\r\n}\r\n\r\n/* 인증번호 확인버튼 */\r\n#IdPw_btnChk1{ /* input */\r\n  width: 420px;\r\n  height: 50px;\r\n  margin: 0 auto;\r\n  border-radius: 5px;\r\n  padding: 10px 15px;\r\n  box-sizing: border-box;\r\n  margin-bottom: 10px;\r\n  margin-right: 20px;\r\n  outline: none;\r\n  border: 1px solid #ccc;\r\n}\r\n#IdPw_btnChk2 {display: none;} /* input */\r\n.IdPw_btnChk label {\r\n  display: block;\r\n  width: 160px;\r\n  height: 50px;\r\n  outline: none;\r\n  float: right;\r\n  border-radius: 5px;\r\n  box-sizing: border-box;\r\n  text-align: center;\r\n  line-height: 50px;\r\n  color: #fff;\r\n  background-color: #002965;\r\n  cursor: pointer;\r\n}\r\n.IdPw_btnChk {\r\n  display: none;\r\n}\r\n\r\n\r\n\r\n/* 중간선 */\r\n.IdPw_bttn_bar {\r\n  width: 600px;\r\n  height: 2px;\r\n  margin: 50px auto; \r\n  border-bottom: 1px solid #ccc;\r\n}\r\n.IdPw_bttn_bar.black {\r\n  border-bottom: 1px solid #000;\r\n}\r\n\r\n\r\n\r\n\r\n@media all and (max-width:1530px) {\r\n  .header {\r\n    width: 1530px;\r\n  }\r\n}"},"lineCount":null}},"error":null,"hash":"b0221967d82ddffb028340fc92cfdcd8","cacheData":{"env":{}}}